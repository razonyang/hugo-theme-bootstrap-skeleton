/* ************************************* SEARCH BAR */
.search-bar {
  height: 100%;
  position: relative;
  text-align: center;
  width: 100%;
}

.search-bar input {
  background: #222;
  background: linear-gradient(#333, #222);
  border: 1px solid #444;
  border-radius: 5px 0 0 5px;
  box-shadow: 0 2px 0 #000;
  color: #888;
  display: block;
  float: left;
  font-size: 13px;
  font-weight: 400;
  height: 40px;
  margin: 0;
  padding: 0 10px;
  text-shadow: 0 -1px 0 #000;
  width: 200px;
}

.ie .search-bar input {
  line-height: 40px;
}

.search-bar input::-webkit-input-placeholder {
  color: #888;
}

.search-bar input:-moz-placeholder {
  color: #888;
}

.search-bar input:focus {
  animation: glow 800ms ease-out infinite alternate;
  background: #292222;
  background: linear-gradient(#393333, #292222);
  border-color: #b40000;
  box-shadow: 0 0 5px rgba(255, 0, 0, 0.2), inset 0 0 5px rgba(255, 0, 0, 0.123), 0 2px 0 #ff0000;
  color: #efe;
  outline: none;
}

.search-bar input:focus::-webkit-input-placeholder {
  color: #ffeeee;
}

.search-bar input:focus:-moz-placeholder {
  color: #ffeeee;
}

.search-bar button {
  background: #222;
  background: linear-gradient(#333, #222);
  box-sizing: border-box;
  border: 1px solid #444;
  border-left-color: #000;
  border-radius: 0 5px 5px 0;
  box-shadow: 0 2px 0 #000;
  color: #fff;
  display: block;
  float: left;
  font-weight: 400;
  height: 40px;
  line-height: 5px;
  text-shadow: 0 -1px 0 #000;
  width: 60px;
}

.search-bar button:hover,
.search-bar button:focus {
  background: #292929;
  background: linear-gradient(#393939, #292929);
  color: #ff5555;
  outline: none;
}

.search-bar button:active {
  background: #292929;
  background: linear-gradient(#393939, #292929);
  box-shadow: 0 1px 0 #000, inset 1px 0 1px #222;
  top: 1px;
}

@keyframes glow {
  0% {
    border-color: #993333;
    box-shadow: 0 0 5px rgba(255, 0, 0, 0.2), inset 0 0 5px rgba(255, 0, 0, 0.1), 0 2px 0 #000;
  }
  100% {
    border-color: #ff6666;
    box-shadow: 0 0 20px rgba(255, 0, 0, 0.6), inset 0 0 10px rgba(255, 0, 0, 0.4), 0 2px 0 #000;
  }
}

.search-bar-side {
  margin-bottom: var(--hbs-component-gutter-b);
  height: 100%;
  position: relative;
  text-align: center;
  width: 100%;
}

.search-bar-side input {
  background: linear-gradient(rgba(253, 19, 19, 0.1), transparent);
  border: 1px solid #444;
  border-radius: 5px 0 0 5px;
  box-shadow: 0 2px 0 #000;
  color: #e35d6a;
  display: block;
  float: left;
  font-size: 13px;
  font-weight: 400;
  height: 40px;
  margin: 0;
  padding: 0 10px;
