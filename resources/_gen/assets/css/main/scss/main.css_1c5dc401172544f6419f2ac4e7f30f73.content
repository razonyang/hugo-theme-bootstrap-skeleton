  width: 200px;
}

.ie .search-bar-side input {
  line-height: 40px;
}

.search-bar-side input::-webkit-input-placeholder {
  color: #e35d6a;
}

.search-bar-side input:-moz-placeholder {
  color: #e35d6a;
}

.search-bar-side input:focus {
  animation: glow 800ms ease-out infinite alternate;
  background: #292222;
  background: linear-gradient(#393333, #292222);
  border-color: #b40000;
  box-shadow: 0 0 5px rgba(255, 0, 0, 0.2), inset 0 0 5px rgba(255, 0, 0, 0.123), 0 2px 0 #ff0000;
  color: #efe;
  outline: none;
}

.search-bar-side input:focus::-webkit-input-placeholder {
  color: red;
}

.search-bar-side input:focus:-moz-placeholder {
  color: red;
}

.search-bar-side button {
  background: #222;
  background: linear-gradient(#333, #222);
